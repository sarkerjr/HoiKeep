generator client {
  provider                  = "prisma-client-js"
  previewFeatures           = ["fieldReference", "clientExtensions"]
  binaryTargets             = ["native"]
  uncheckedRelationalInputs = "true"
  uncheckedScalarInputs     = "true"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Hall {
  id      String @id @default(uuid())
  name    String
  nameTag String
}
